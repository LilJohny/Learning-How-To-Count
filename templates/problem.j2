{% extends "main.j2" %}

{% block main %}
 <div class="overlay"></div>
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-md-10 mx-auto">
                <div class="post-heading">
                    <h1>{{ title}}</h1>
                    <h2 class="subheading">{{ subtitle }}</h2>
                </div>
            </div>
        </div>
    </div>
</header>

<!-- Post Content -->
<article>
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-md-10 mx-auto">
                <iframe width="100%" height="450" src="https://www.youtube.com/embed/mkyZ45KQYi4?list=PLC58778F28211FA19" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
                <p>
                    {{ text }}
                <p>
                    <br>
                </p>
            </div>

        </div>
    </div>
</article>

<hr>
{% endblock %}